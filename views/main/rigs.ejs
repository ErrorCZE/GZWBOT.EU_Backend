<!DOCTYPE html>
<html>

<head>
  <title>Gray Zone Warfare Bot - Rigs</title>
  <link rel="canonical" href="https://gzwbot.eu/rigs" />
  <link rel="stylesheet" href="/css/main.css?v=<%= cssVersion %>">
<script src="/js/main.js"></script>
  <meta name="description" content="Explore our comprehensive collection of armored rigs and tactical rigs! Discover gear designed for both protection and functionality. From lightweight carriers to heavy-duty loadouts, find the perfect fit for your mission. Compare stats like weight, size, price, armor class, and material.">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="theme-color" content="#28B828">
  <link rel="icon" href="/images/web/favicon.ico" type="image/x-icon">
  <link rel="icon" href="/images/web/favicon.png" type="image/png">
  <meta name="keywords" content="gzw, gray, zone, warfare, wiki, rig, rigs">
  <meta name="subject" content="Gray Zone Warfare">
  <meta name='coverage' content='Worldwide'>
  <meta name='distribution' content='Global'>
  <meta name='rating' content='General'>
  <meta name='target' content='all'>
  <meta name="audience" content="all" />
  <meta name="topic" content="Gray Zone Warfare Rigs">
  <meta name="summary" content="Explore our comprehensive collection of armored rigs and tactical rigs! Discover gear designed for both protection and functionality. From lightweight carriers to heavy-duty loadouts, find the perfect fit for your mission. Compare stats like weight, size, price, armor class, and material.">
  <meta property="og:image" content="https://gzwbot.eu/images/web/gzwlogo.webp">
  <meta property="og:title" content="Gray Zone Warfare Bot - Rigs">
  <meta property="og:description" content="Explore our comprehensive collection of armored rigs and tactical rigs! Discover gear designed for both protection and functionality. From lightweight carriers to heavy-duty loadouts, find the perfect fit for your mission. Compare stats like weight, size, price, armor class, and material.">
  <meta name="twitter:image" content="https://gzwbot.eu/images/web/gzwlogo.webp">
  <meta name="twitter:title" content="Gray Zone Warfare Tools - Rigs">
  <meta name="twitter:description" content="Explore our comprehensive collection of armored rigs and tactical rigs! Discover gear designed for both protection and functionality. From lightweight carriers to heavy-duty loadouts, find the perfect fit for your mission. Compare stats like weight, size, price, armor class, and material.">

  <script src="/js/sort.js"></script>
  <script src="/js/tableMenu.js"></script>
</head>

<body>
  <%- include('./basic/header') -%>

  <div class="container">
    <h1 class="mainheader">Tactical Rigs</h1>

    <div class="tableMenu">
      <div class="item active" onclick="showTable('tacticalRigs')" data-item-id="tacticalRigs">Tactical rigs</div>
      <div class="item" onclick="showTable('armorPlateCarriers')" data-item-id="armorPlateCarriers">Armor Plate Carriers</div>
    </div>

    <div class="tableContainer" id="tacticalRigsTable">
      <h2>Tactical Rigs</h2>
      <table class="table table-striped table-sm">
        <thead>
          <tr>
            <th scope="col">Icon</th>
            <th scope="col" style="text-align: left;">Name</th>
            <th scope="col" onclick="sortTable('tacticalRigsTable', 2)">Weight</th>
            <th scope="col" onclick="sortTable('tacticalRigsTable', 3)">Inner Size</th>
            <th scope="col" onclick="sortTable('tacticalRigsTable', 4)">Size</th>
            <th scope="col" onclick="sortTable('tacticalRigsTable', 5)">Buy</th>
            <th scope="col" onclick="sortTable('tacticalRigsTable', 6)">Sell</th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < tacticalRigs.length; i++) { %>
          <tr>
            <td><img class="itemIcon" src="/images/items/<%= tacticalRigs[i].imageName || "/notfound.webp"  %>"></td>
            <td style="text-align: left;"><%= tacticalRigs[i].name %></td>
            <td><%= JSON.stringify(tacticalRigs[i].properties.weight) %>kg</td>
            <td><%= JSON.stringify(tacticalRigs[i].properties.innerSize.X) %>x<%= JSON.stringify(tacticalRigs[i].properties.innerSize.Y) %></td>
            <td><%= JSON.stringify(tacticalRigs[i].properties.size.X) %>x<%= JSON.stringify(tacticalRigs[i].properties.size.Y) %></td>
            <td><%= JSON.stringify(tacticalRigs[i].properties.price) %></td>
            <td><%= Math.round(tacticalRigs[i].properties.price * tacticalRigs[i].properties.sellRate) %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>

    <div class="tableContainer" id="armorPlateCarriersTable" style="display: none">
      <h2>Armor Plate Carriers</h2>
      <table class="table table-striped table-sm">
        <thead>
          <tr>
            <th scope="col">Icon</th>
            <th scope="col" style="text-align: left;">Name</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 2)">Weight</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 3)">Inner Size</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 4)">Size</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 5)">Overall Joules</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 6)">Min. Dmg. Joules</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 7)">Plates</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 8)">Class</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 9)">Material</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 10)">Buy</th>
            <th scope="col" onclick="sortTable('armorPlateCarriersTable', 11)">Sell</th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < armorPlateCarriers.length; i++) { %>
          <tr>
            <td><img class="itemIcon" src="/images/items/<%= armorPlateCarriers[i].imageName || "/notfound.webp"  %>"></td>
            <td style="text-align: left;"><%= armorPlateCarriers[i].name %></td>
            <td><%= JSON.stringify(armorPlateCarriers[i].properties.weight) %>kg</td>
            <td><%= JSON.stringify(armorPlateCarriers[i].properties.innerSize.X) %>x<%= JSON.stringify(armorPlateCarriers[i].properties.innerSize.Y) %></td>
            <td><%= JSON.stringify(armorPlateCarriers[i].properties.size.X) %>x<%= JSON.stringify(armorPlateCarriers[i].properties.size.Y) %></td>
            <td><%= JSON.stringify(armorPlateCarriers[i].properties.overallJoules) %>J</td>
            <td><%= JSON.stringify(armorPlateCarriers[i].properties.minimumDamageJoules) || "0" %>J</td>
            <td><%= armorPlateCarriers[i].properties.plates.join(', ') %></td>
            <td><%= armorPlateCarriers[i].properties.class %></td>
            <td><%= armorPlateCarriers[i].properties.material %></td>
            <td><%= JSON.stringify(armorPlateCarriers[i].properties.price) %></td>
            <td><%= Math.round(armorPlateCarriers[i].properties.price * armorPlateCarriers[i].properties.sellRate) %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>

  </div>

  <%- include('./basic/footer') -%>
</body>

</html>