<!DOCTYPE html>
<html>

<head>
  <title>Gray Zone Warfare Bot - Grenades</title>
  <link rel="canonical" href="https://gzwbot.eu/grenades" />
  <link rel="stylesheet" href="/css/main.css?v=<%= cssVersion %>">
<script src="/js/main.js"></script>
  <meta name="description" content="Explore an arsenal of Grenades">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="theme-color" content="#28B828">
  <link rel="icon" href="/images/web/favicon.ico" type="image/x-icon">
  <link rel="icon" href="/images/web/favicon.png" type="image/png">
  <meta name="keywords" content="gzw, gray, zone, warfare, wiki, info, grenades, grenade">
  <meta name="subject" content="Gray Zone Warfare">
  <meta name='coverage' content='Worldwide'>
  <meta name='distribution' content='Global'>
  <meta name='rating' content='General'>
  <meta name='target' content='all'>
  <meta name="audience" content="all" />
  <meta name="topic" content="Gray Zone Warfare Grenades">
  <meta name="summary" content="Explore an arsenal of Grenades">
  <meta property="og:image" content="https://gzwbot.eu/images/web/gzwlogo.webp">
  <meta property="og:title" content="Gray Zone Warfare Bot - Grenades">
  <meta property="og:description" content="Explore an arsenal of Grenades">
  <meta name="twitter:image" content="https://gzwbot.eu/images/web/gzwlogo.webp">
  <meta name="twitter:title" content="Gray Zone Warfare Tools - Grenades">
  <meta name="twitter:description" content="Explore an arsenal of Grenades">

  <script src="/js/sort.js"></script>
  <script src="/js/tableMenu.js"></script>
</head>

<body>
  <%- include('./basic/header') -%>

  <div class="container">
    <h1 class="mainheader">Grenades</h1>

    <div class="tableContainer" id="grenadesTable">
      <table class="table table-striped table-sm">
        <thead>
          <tr>
            <th scope="col">Icon</th>
            <th scope="col" style="text-align: left;">Name</th>
            <th scope="col" onclick="sortTable('grenadesTable', 2)">Type</th>
            <th scope="col" onclick="sortTable('grenadesTable', 3)">Weight</th>
            <th scope="col" onclick="sortTable('grenadesTable', 4)">Fuse Time Min</th>
            <th scope="col" onclick="sortTable('grenadesTable', 5)">Fuse Time Max</th>
            <th scope="col" onclick="sortTable('grenadesTable', 6)">Arm Time</th>
            <th scope="col" onclick="sortTable('grenadesTable', 7)">Fragments</th>
            <th scope="col" onclick="sortTable('grenadesTable', 8)">Kill Radius</th>
            <th scope="col" onclick="sortTable('grenadesTable', 9)">Wound Radius</th>
            <th scope="col" onclick="sortTable('grenadesTable', 10)">Stun Rad. Min</th>
            <th scope="col" onclick="sortTable('grenadesTable', 11)">Stun Rad. Max</th>
            <th scope="col" onclick="sortTable('grenadesTable', 12)">Buy</th>
            <th scope="col" onclick="sortTable('grenadesTable', 13)">Sell</th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < grenades.length; i++) { %>
          <tr>
            <td><img class="itemIcon" src="/images/items/<%= grenades[i].imageName || "/notfound.webp"  %>"></td>
            <td style="text-align: left;"><%= grenades[i].name %></td>
            <td><%= grenades[i].properties.grenadeType %></td>
            <td><%= JSON.stringify(grenades[i].properties.weight) %>kg</td>
            <td><%= JSON.stringify(grenades[i].properties.fuseTimeMin) %></td>
            <td><%= JSON.stringify(grenades[i].properties.fuseTimeMax) %></td>
            <td><%= JSON.stringify(grenades[i].properties.armTime) %></td>
            <td><%= JSON.stringify(grenades[i].properties.fragments) %></td>
            <td><%= JSON.stringify(grenades[i].properties.killRadius) %></td>
            <td><%= JSON.stringify(grenades[i].properties.woundRadius) %></td>
            <td><%= JSON.stringify(grenades[i].properties.stunRadiusMin) %></td>
            <td><%= JSON.stringify(grenades[i].properties.stunRadiusMax) %></td>
            <td><%= JSON.stringify(grenades[i].properties.price) %></td>
            <td><%= Math.round(grenades[i].properties.price * grenades[i].properties.sellRate) %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>

  </div>

  <%- include('./basic/footer') -%>
</body>

</html>