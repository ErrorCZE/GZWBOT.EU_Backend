<!DOCTYPE html>
<html>

<head>
  <title>Gray Zone Warfare Bot - Backpacks</title>
  <link rel="canonical" href="https://gzwbot.eu/backpacks" />
  <link rel="stylesheet" href="/css/main.css?v=<%= cssVersion %>">
<script src="/js/main.js"></script>
  <meta name="description" content="Explore a diverse range of backpacks within the game and find the perfect fit to meet all your needs.">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="theme-color" content="#28B828">
  <link rel="icon" href="/images/web/favicon.ico" type="image/x-icon">
  <link rel="icon" href="/images/web/favicon.png" type="image/png">
  <meta name="keywords" content="gzw, gray, zone, warfare, wiki, backpacks, backpack">
  <meta name="subject" content="Gray Zone Warfare Backpacks">
  <meta name='coverage' content='Worldwide'>
  <meta name='distribution' content='Global'>
  <meta name='rating' content='General'>
  <meta name='target' content='all'>
  <meta name="audience" content="all" />
  <meta name="topic" content="Gray Zone Warfare Backpacks">
  <meta name="summary" content="Explore a diverse range of backpacks within the game and find the perfect fit to meet all your needs.">
  <meta property="og:image" content="https://gzwbot.eu/images/web/gzwlogo.webp">
  <meta property="og:title" content="Gray Zone Warfare Bot - Backpacks">
  <meta property="og:description" content="Explore a diverse range of backpacks within the game and find the perfect fit to meet all your needs.">
  <meta name="twitter:image" content="https://gzwbot.eu/images/web/gzwlogo.webp">
  <meta name="twitter:title" content="Gray Zone Warfare Tools - Backpacks">
  <meta name="twitter:description" content="Explore a diverse range of backpacks within the game and find the perfect fit to meet all your needs.">

  <script src="/js/sort.js"></script>
  <script src="/js/tableMenu.js"></script>
</head>

<body>
  <%- include('./basic/header') -%>

  <div class="container">
    <h1 class="mainheader">Backpacks</h1>

    <div class="tableContainer" id="backpacksTable">
      <table class="table table-striped table-sm">
        <thead>
          <tr>
            <th scope="col">Icon</th>
            <th scope="col" style="text-align: left;">Name</th>
            <th scope="col" onclick="sortTable('backpacksTable', 2)">Weight</th>
            <th scope="col" onclick="sortTable('backpacksTable', 3)">Inner Size</th>
            <th scope="col" onclick="sortTable('backpacksTable', 4)">Size</th>
            <th scope="col" onclick="sortTable('backpacksTable', 5)">Weapon Slot</th>
            <th scope="col" onclick="sortTable('backpacksTable', 6)">Buy</th>
            <th scope="col" onclick="sortTable('backpacksTable', 7)">Sell</th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < backpacks.length; i++) { %>
          <tr>
            <td><img class="itemIcon" src="/images/items/<%= backpacks[i].imageName || "/notfound.webp"  %>"></td>
            <td style="text-align: left;"><%= backpacks[i].name %></td>
            <td><%= JSON.stringify(backpacks[i].properties.weight) %>kg</td>
            <td><%= JSON.stringify(backpacks[i].properties.innerSize.X) %>x<%= JSON.stringify(backpacks[i].properties.innerSize.Y) %></td>
            <td><%= JSON.stringify(backpacks[i].properties.size.X) %>x<%= JSON.stringify(backpacks[i].properties.size.Y) %></td>
            <td><%= backpacks[i].properties.weaponSlot ? "Yes" : "No" %></td>
            <td><%= JSON.stringify(backpacks[i].properties.price) %></td>
            <td><%= Math.round(backpacks[i].properties.price * backpacks[i].properties.sellRate) %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>

  </div>

  <%- include('./basic/footer') -%>
</body>

</html>